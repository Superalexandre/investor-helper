{
    "compilerOptions": {
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ES2022",
            "ESNext"
        ],
        "target": "ES2022",
        "module": "ESNext",
        "skipLibCheck": true,
        "incremental": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "jsx": "react-jsx",
        "moduleResolution": "node16",
        "resolveJsonModule": true,
        "strict": true,
        "allowJs": true,
        "forceConsistentCasingInFileNames": true,
        "baseUrl": ".",
        "paths": {
            "@/*": ["*"]
        }
    },
    "include": [
        "worker.ts",
        "server.ts"
    ],
    "exclude": [
        "node_modules",
        "build"
    ]
}